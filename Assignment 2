#include <stdio.h>
int main () {
int age , i;
printf("Enter your age ");
scanf("%d",&age);
printf("Enter the number 1 if you are Indian, and 0 if are not Indian\n");
scanf("%d",&i);
if (age>=18 && i==1){
printf("You are eligible to vote");
}
else if (age<18 || i==0){
printf("You are not eligible to vote ");
}
else {
printf("Enter the valid number");
}
return 0;
}
