#include<stdio.h>
int main () {
int phy, chem, maths, bio, comp;
printf ("Enter the marks of the 5 subjects\n");
scanf ("%d%d%d%d%d", &phy, &chem, &maths, &bio, &comp);
float per, tot;
tot = phy + chem + maths + bio + comp;
per = tot/5;
printf ("Your total percentage is : %f\n", per);
if (per >=90) {
printf ("Your grade is A");
}
else if (per >=80) {
printf ("Your grade is B");
}

else if (per >=70) {
printf ("Your grade is C");
}
else if (per >=60) {
printf ("Your grade is D");
}
else if (per >= 40) {
printf ("Your grade is E");
}
else {
printf ("Your grade is F");
}
return 0;
}
